<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name = "seekbarWithText">
        <attr name = "titleText" format = "string" localization = "suggested"></attr>
        <attr name = "sound" format = "integer"></attr>
        <attr name = "playsound" format = "boolean"></attr>
        <attr name = "stream" format = "integer">
            <enum name = "voice" value = "0"></enum>
            <enum name = "system" value = "1"></enum>
            <enum name = "ring" value = "2"></enum>
            <enum name = "music" value = "3"></enum>
            <enum name = "alarm" value = "4"></enum>
            <enum name = "notification" value = "5"></enum>
            <enum name = "touch" value = "8"></enum>
        </attr>
    </declare-styleable>
</resources>
